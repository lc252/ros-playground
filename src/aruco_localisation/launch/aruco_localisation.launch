<!-- call launch files from within this launch file, see answers.ros.org/question/9426/can-i-call-one-launch-file-from-another-launch-file/ -->

<launch>
    <include file="$(find aruco_localisation)/launch/aruco_detect.launch"/>
    <include file="$(find aruco_localisation)/launch/static_fiducial_tfs.launch"/>
    <node pkg="aruco_localisation" name="aruco_tf" type="reverse_aruco_tf"></node>
    <node pkg="rviz" name="rviz" type="rviz" args="-d $(find aruco_localisation)/rviz/settings.rviz" required="true"></node>
    <node pkg="depth_camera" name="camera_stream" type="realsense_camera"></node>
</launch>